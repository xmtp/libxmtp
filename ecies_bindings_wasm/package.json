{
  "name": "@xmtp/ecies-bindings-wasm",
  "collaborators": [
    "XMTP Engineering <eng@xmtp.com>"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:browser && npm run build:node && npm run clean:gitignore",
    "build:browser": "wasm-pack build --target bundler --no-pack --release --out-dir ./dist/browser",
    "build:node": "wasm-pack build --target nodejs --no-pack --release --out-dir ./dist/node",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./dist",
    "clean:gitignore": "rm -f ./dist/**/.gitignore"
  },
  "version": "0.1.5",
  "files": [
    "./dist/**/*"
  ],
  "main": "dist/node/ecies_bindings_wasm.js",
  "types": "dist/browser/ecies_bindings_wasm.d.ts",
  "browser": "dist/browser/ecies_bindings_wasm.js",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "wasm-pack": "^0.12.1"
  }
}
